{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=AutismCenter;Username=postgres;Password=password;Pooling=true;MinPoolSize=5;MaxPoolSize=100;ConnectionIdleLifetime=300;ConnectionPruningInterval=10;",
    "Redis": "localhost:6379"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-key-for-performance-testing-environment",
    "Issuer": "AutismCenter",
    "Audience": "AutismCenter-Users",
    "ExpiryMinutes": 60,
    "RefreshTokenExpiryDays": 7
  },
  "PerformanceSettings": {
    "EnableCaching": true,
    "CacheExpiryMinutes": 60,
    "EnableCompression": true,
    "EnableResponseCaching": true,
    "MaxConcurrentRequests": 500,
    "RequestTimeoutSeconds": 30
  },
  "DatabaseSettings": {
    "CommandTimeout": 30,
    "EnableRetryOnFailure": true,
    "MaxRetryCount": 3,
    "MaxRetryDelay": "00:00:05",
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false
  }
}